<?xml version="1.0" encoding="utf-8"?>
<manifest package="gurungsijan.com.cleanapp_music"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.DemoMusicApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".artists.ui.activity.ArtistSearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".albums.ui.activity.AlbumActivity">
        </activity>
        <activity android:name=".albums.ui.activity.AlbumDetailActivity">
        </activity>
    </application>

</manifest>